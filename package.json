{
  "name": "ui-core",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@soxo/bootstrap-core": "file:../bootstrap-core",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.51.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
